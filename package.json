{
  "name": "acf-dropzone",
  "version": "1.0.0",
  "description": "Drag and drop upload for ACF Fields",
  "private": true,
  "scripts": {
    "dev": "gulp dev",
    "i18n": "wp i18n make-pot . languages/acf-dropzone.pot --domain=acf-dropzone",
    "rollback": "git reset --hard HEAD~ && git push origin +master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcguffin/acf-dropzone.git"
  },
  "author": "JÃ¶rn Lund",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mcguffin/acf-dropzone/issues"
  },
  "browserslist": [
    "last 3 versions",
    "> 0.25%",
    "IE 10"
  ],
  "homepage": "https://github.com/mcguffin/acf-dropzone#readme",
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "*",
    "gulp-concat": "*",
    "gulp-rename": "*",
    "gulp-sass": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "*",
    "gulp-util": "*"
  },
  "dependencies": {},
  "wporg": {
    "type": "plugin",
    "steps": [
      "build",
      "github",
      "wporg"
    ],
    "build": {
      "prebuild": [
        "wp i18n make-pot . languages/acf-dropzone.pot --domain=acf-dropzone"
      ],
      "build": [
        "gulp build"
      ]
    },
    "github": {
      "user": "mcguffin",
      "host": "github.com"
    },
    "wporg": {
      "svn": "https://plugins.svn.wordpress.org/acf-dropzone/",
      "assets": ".wporg"
    }
  }
}
